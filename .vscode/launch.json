{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "chrome",
      "request": "launch",
      "skipFiles": ["${workspaceFolder}/node_modules/**/*.js"],
      "name": "Launch Chrome(localhost)",
      "url": "http://localhost:3333",
      "webRoot": "${workspaceFolder}/src",
      "runtimeArgs": ["--remote-debugging-port=9222"],
      "sourceMaps": true
    }
  ]
}
